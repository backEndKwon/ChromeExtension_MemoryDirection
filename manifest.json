{
  "manifest_version": 3,
  "name": "Let's Memory Direction Game",
  "description": "This extension allows you to play a memory direction game",
  "version": "1.5",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "128.png"
    }
  },
  "background": {
    "service_worker": "index.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ]
}
